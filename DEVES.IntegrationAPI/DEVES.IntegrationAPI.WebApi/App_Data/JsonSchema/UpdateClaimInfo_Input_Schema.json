{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"ticketNo": {
      "type": "string",
      "minLength": 1,
			"maxLength": 50
		},
		"claimNotiNo": {
      "type": "string",
      "minLength": 1,
			"maxLength": 15
		},
		"claimNo": {
			"type": "string",
			"maxLength": 8
		},
		"claimStatusCode": {
      "type": "string",
      "minLength": 1,
			"maxLength": 15
		},
		"claimStatusDesc": {
      "type": "string",
      "minLength": 1,
			"maxLength": 100
		},
		"vipCaseFlag": {
      "type": "string",
      "enum": [
        "Y",
        "N"
      ],
      "minLength": 1,
			"maxLength": 1
		},
		"highLossCaseFlag": {
			"type": "string",
      "enum": [
        "Y",
        "N"
      ],
      "minLength": 1,
			"maxLength": 1
		},
		"legalCaseFlag": {
			"type": "string",
      "enum": [
        "Y",
        "N"
      ],
      "minLength": 1,
			"maxLength": 1
		}
	},
	"required": [
		"ticketNo",
		"claimNotiNo",
		"claimStatusCode",
    "claimStatusDesc",
    "vipCaseFlag",
    "highLossCaseFlag",
    "legalCaseFlag"
	]
}