{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {

    "ticketNo": {
      "type": "string",
      "maxLength": 50
    },
    "claimNotiNo": {
      "type": "string",
      "maxLength": 15
    },
    "eventId": {
      "type": "string",
      "maxLength": 20
    },
    "caseOwnerCode": {
      "type": "string",
      "maxLength": 20
    },
    "caseOwnerFullName": {
      "type": "string",
      "maxLength": 250
    },
    "reportAccidentResultDate": {
      "type": "string"
    },

    "eventDetailInfo": {
      "type": "object",
      "items": {
        "type": "object",
        "properties": {
          "accidentOn": {
            "type": "string",
            "maxLength": 20
          },
          "accidentLatitude": {
            "type": "string",
            "maxLength": 20
          },
          "accidentLongitude": {
            "type": "string",
            "maxLength": 20
          },
          "accidentPlace": {
            "type": "string",
            "maxLength": 2000
          },
          "accidentNatureDesc": {
            "type": "string",
            "maxLength": 2000
          },
          "accidentRemark": {
            "type": "string",
            "maxLength": 2000
          },
          "accidentLegalResult": {
            "type": "string",
            "maxLength": 1
          },
          "policeStation": {
            "type": "string",
            "maxLength": 200
          },
          "policeRecordId": {
            "type": "string",
            "maxLength": 200
          },
          "policeRecordDate": {
            "type": "string"
          },
          "policeBailFlag": {
            "type": "string",
            "maxLength": 1
          },
          "numOfTowTruck": {
            "type": "integer"
          },
          "numOfAccidentInjury": {
            "type": "integer"
          },
          "numOfDeath": {
            "type": "integer"
          },
          "deductibleFee": {
            "type": "integer"
          },
          "excessFee": {
            "type": "integer"
          },
          "totalEvent": {
            "type": "string",
            "maxLength": 20
          },
          "iSurveyCreatedDate": {
            "type": "string",
            "maxLength": 20
          },
          "iSurveyModifiedDate": {
            "type": "string",
            "maxLength": 20
          },
          "iSurveyIsDeleted": {
            "type": "string",
            "maxLength": 1
          },
          "iSurveyIsDeletedDate": {
            "type": "string",
            "maxLength": 20
          },
          "numOfAccidentParty": {
            "type": "integer"
          }
        },
        "required": [
          "accidentOn",
          "accidentLegalResult",
          "numOfTowTruck",
          "numOfAccidentInjury",
          "numOfDeath",
          "iSurveyCreatedDate",
          "iSurveyModifiedDate",
          "iSurveyIsDeleted",
          "iSurveyIsDeletedDate",
          "numOfAccidentParty"
        ]
      }
    },

    "partiesInfo": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "partiesEventId": {
            "type": "string",
            "maxLength": 20
          },
          "partiesEventItem": {
            "type": "string",
            "maxLength": 20
          },
          "partiesId": {
            "type": "string",
            "maxLength": 50
          },
          "partiesFullname": {
            "type": "string",
            "maxLength": 200
          },
          "partiesType": {
            "type": "integer"
          },
          "partiesCarPlateNo": {
            "type": "string",
            "maxLength": 50
          },
          "partiesCarPlateProv": {
            "type": "string",
            "maxLength": 50
          },
          "partiesCarBrand": {
            "type": "string",
            "maxLength": 50
          },
          "partiesCarColor": {
            "type": "string",
            "maxLength": 50
          },
          "partiesPartyPhone": {
            "type": "string",
            "maxLength": 50
          },
          "partiesInsuranceCompany": {
            "type": "string",
            "maxLength": 200
          },
          "partiesPolicyNumber": {
            "type": "string",
            "maxLength": 50
          },
          "partiesPolicyType": {
            "type": "string",
            "maxLength": 200
          },
          "partiesCreatedDate": {
            "type": "string",
            "maxLength": 20
          },
          "partiesModifiedDate": {
            "type": "string",
            "maxLength": 20
          },
          "partiesIsDeleted": {
            "type": "string",
            "maxLength": 1
          },
          "partiesIsDeletedDate": {
            "type": "string",
            "maxLength": 20
          }
        },
        "required": [
          "partiesFullname",
          "partiesType",
          "partiesCreatedDate",
          "partiesModifiedDate",
          "partiesIsDeleted",
          "partiesIsDeletedDate"
        ]
      }
    },

    "claimDetailInfo": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "claimDetailEventId": {
            "type": "string",
            "maxLength": 20
          },
          "claimDetailItem": {
            "type": "string",
            "maxLength": 20
          },
          "claimDetailDetailid": {
            "type": "string",
            "maxLength": 20
          },
          "claimDetailDetail": {
            "type": "string",
            "maxLength": 255
          },
          "claimDetailLevels": {
            "type": "string",
            "maxLength": 1
          },
          "claimDetailIsRepair": {
            "type": "string",
            "maxLength": 1
          },
          "claimDetailRemark": {
            "type": "string",
            "maxLength": 2000
          },
          "claimDetailCreatedDate": {
            "type": "string",
            "maxLength": 20
          },
          "claimDetailModifiedDate": {
            "type": "string",
            "maxLength": 20
          },
          "claimDetailIsDeleted": {
            "type": "string",
            "maxLength": 1
          },
          "claimDetailIsDeletedDate": {
            "type": "string",
            "maxLength": 20
          }
        },
        "required": [
          "claimDetailCreatedDate",
          "claimDetailModifiedDate",
          "claimDetailIsDeleted",
          "claimDetailIsDeletedDate"
        ]
      }
    },

    "claimDetailPartiesInfo": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "claimDetailPartiesPartiesId": {
            "type": "string",
            "maxLength": 20
          },
          "claimDetailPartiesItem": {
            "type": "string",
            "maxLength": 20
          },
          "claimDetailPartiesDetailId": {
            "type": "string",
            "maxLength": 20
          },
          "claimDetailPartiesDetail": {
            "type": "string",
            "maxLength": 255
