{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
  "properties": {
    "clientType": {
      "type": "string",
      "enum": [
        "P",
        "C"
      ],
      "minLength": 1,
      "maxLength": 1
    },
    "roleCode": {
      "type": "string",
      "enum": [
        "G",
        "A",
        "S",
        "R",
        "H"
      ],
      "minLength": 1,
      "maxLength": 5
    },
    "polisyClientId": {
      "type": "string",
      "maxLength": 8
    },
    "sapVendorCode": {
      "type": "string",
      "maxLength": 10
    },
    "fullname": {
      "type": "string",
      "maxLength": 100
    },
    "taxNo": {
      "type": "string",
      "maxLength": 13
    },
    "taxBranchCode": {
      "type": "string",
      "maxLength": 18
    },
    "requester": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "emcsCode": {
      "type": "string",
      "maxLength": 10
    }
  },
	"required": [
    "clientType",
    "roleCode",
    "requester"
	]
}