{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "caseNo": {
      "type": "string",
      "maxLength": 50
    },
    "tempID": {
      "type": "integer"
    },
    "complaintNo": {
      "type": "string",
      "maxLength": 10
    },
    "complaintStatus": {
        "anyOf": [{
                "type": "string",
                "enum": [
                    "1",
                    "2",
                    "3"                ],
                "maxLength": 1
            },
            {
                "type": "null"
            }
        ]
    },
    "complaintStep": {
        "anyOf": [{
                "type": "string",
                "enum": [
                    "01",
                    "02",
                    "31",
                    "32",
                    "33",
                    "34",
                    "35",
                    "04",
                    "05"
                ],
                "maxLength": 2
            },
            {
                "type": "null"
            }
        ]
    },
    "complaintStepdate": {
      "type": "string"
    }
  },
  "required": [
    "caseNo"
	]
}