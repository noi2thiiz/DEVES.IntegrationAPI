  "properties": {
    "ticketNo": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "claimNotiNo": {
      "type": "string",
      "minLength": 1,
      "maxLength": 15
    },
    "claimNo": {
      "type": "string",
      "maxLength": 8
    },
    "claimId": {
      "type": "string",
      "maxLength": 20
    },
    "claimStatusCode": {
      "type": "string",
      "minLength": 1,
      "maxLength": 5
    },
    "claimStatusDesc": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100
    }
  },
  "required": [
    "ticketNo",
    "claimNotiNo",
    "claimStatusCode",
    "claimStatusDesc"
  ]
