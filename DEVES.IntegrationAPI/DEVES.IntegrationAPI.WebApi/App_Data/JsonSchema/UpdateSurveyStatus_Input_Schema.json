{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
    "ticketNo": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "claimNotiNo": {
      "type": "string",
      "minLength": 1,
      "maxLength": 15,
    },
    "iSurveyStatus": {
      "type": "string",
      "enum": [
        "09",
        "10",
        "15",
        "17",
        "20",
        "40",
        "90",
        "70",
        "71",
        "99"
      ],
      "minLength": 2,
      "maxLength": 2
    },
    "iSurveyStatusOn": {
      "type": "string",
      "minLength": 1
    }
	},
	"required": [
    "ticketNo",
    "claimNotiNo",
    "iSurveyStatus",
    "iSurveyStatusOn"
	]
}