function AppCommonUtil($,Xrm){
    var targetDocument = window.top.document;

    function getErrorImg() {

        return "data:image/png;base64,";

    }
    function getSuccessImg() {
        return 'data:image/png;base64,';
    }


    function OpenDialog(option) {

        var cfg = $.extend({
            title: "Alert box",
            type: "alert",

            document:document,
            success: function (result) {
            },
            beforeShow: function () {
            },
            afterShow: function () {
            },
            beforeClose: function () {
            },
            afterClose: function () {
            },
        }, option);

        cfg.beforeShow();


        if($("body", targetDocument).find('#dialogAlertDivOverlay').length !=0){
            
            $("body", targetDocument).find('#dialogAlertDivOverlay').remove();
            
        }


            var elm = $('<div id="dialogAlertDivOverlay"><div  id="dialogAlertDiv" ><p  style="font-size: large;height:10px;"><img id="dialogAlertImg" style=" width: 25px; vertical-align: middle;"/>'+cfg.title+'</p><hr/><div style="height:100px;" >'+cfg.content+'</div><div style="margin-top: 5px;"><hr/><button style="float: right;" id="alertOkButton">OK</button></div></div></div>');

            if(cfg.type=='success'){

                elm.find("#dialogAlertImg").attr("src", getSuccessImg() );

            }else {

                elm.find("#dialogAlertImg").attr("src", getErrorImg() );
            }

            elm.css('visibility', 'visible');
            elm.css('position', 'absolute');
            elm.css('left', '0px');
            elm.css('top', '0px');
            elm.css('width', screen.width);
            elm.css('font-size', '1.5em');
            elm.css('height', screen.height);
            elm.css('text-align', 'center');
            elm.css('z-index', '1000');
            elm.css('background-color', 'rgba(224, 224, 224, 0.25)');


            elm.css('height', '100%');
            elm.css('margin', '0');
            elm.css('padding', '0');

            elm.find('#dialogAlertDiv').css('width', '500px');
            elm.find('#dialogAlertDiv').css('height', '200px');
            elm.find('#dialogAlertDiv').css('margin', '200px auto');
            elm.find('#dialogAlertDiv').css('background-color', '#fff');
            elm.find('#dialogAlertDiv').css('border', '1px solid #000');
            elm.find('#dialogAlertDiv').css('padding', '15px');
            elm.find('#dialogAlertDiv').css('padding-top', '0');
            elm.find('#dialogAlertDiv').css('text-align', 'center');

            $("body", targetDocument).append(elm);
       



        $("body", targetDocument).find('#dialogAlertDivOverlay').css('visibility', 'visible');
        cfg.afterShow();
        $("body", targetDocument).find('#dialogAlertDivOverlay').find("#alertOkButton").unbind().bind("click",function () {
            cfg.beforeClose();
            $("body", targetDocument).find('#dialogAlertDivOverlay').css('visibility', 'hidden');
            cfg.afterClose();
        })

        cfg.success();
        //$("body", window.top.document).find('#dialogAlertDivOverlay').css('visibility', 'visible');
    }
    function CloseDialog(){
        $("body", targetDocument).find('#dialogAlertDivOverlay').css('visibility', 'hidden');
        $("body", targetDocument).find('#dialogAlertDivOverlay').remove();
    }
    function LoadingStart() {

        if($("body", targetDocument).find('#dialogDiv').length ==0){


            var elm = $('<div id="overlay"><div id="dialogDiv"><p>Please wait</p><img src="data:image/gif;base64," /></div></div>');
            elm.css('visibility', 'hidden');
            elm.css('position', 'absolute');
            elm.css('left', '0px');
            elm.css('top', '0px');
            elm.css('width', screen.width);
            elm.css('height', screen.height);
            elm.css('text-align', 'center');
            elm.css('z-index', '1000');
            elm.css('background-color', 'rgba(224, 224, 224, 0.25)');


            elm.css('height', '100%');
            elm.css('margin', '0');
            elm.css('padding', '0');

            elm.find('#dialogDiv').css('width', '500px');
            elm.find('#dialogDiv').css('height', '200px');
            elm.find('#dialogDiv').css('margin', '200px auto');
            //  elm.find('#dialogDiv').css('background-color', '#fff');
            //  elm.find('#dialogDiv').css('border', '1px solid #000');
            elm.find('#dialogDiv').css('padding', '15px');
            elm.find('#dialogDiv').css('text-align', 'center');


            $("body", window.top.document).append(elm);
        }

        $("body", window.top.document).find('#overlay').css('visibility', 'visible');
    }

    function LoadingStop() {



        $("body", targetDocument).find('#overlay').css('visibility', 'hidden');
    }

    function strPad(str,pad) {

        if(!str){
            str = "";
        }
        str = ""+str;

        return  pad.substring(0, pad.length - str.length) + str
    }
    function formatDate(date) {
        //2017-01-01 09:10:11
        var hours =  date.getHours();
        var minutes = date.getMinutes();
        var seconds = date.getSeconds();

        var year = strPad(date.getFullYear(),"0000");
        var month = strPad(date.getMonth()+1,"00")
        var day = strPad(date.getDate(),"00")


        var strTime = strPad(hours,"00") + ':' + strPad(minutes,"00")+":"+strPad(seconds,"00");
        return  year+"-"+ month+ "-" +  day  + " " + strTime;
    }

    function getOdataObject(object, callback) {
        var serverUrl = window.parent.Xrm.Page.context.getClientUrl();
        var ODATA_ENDPOINT = "/XRMServices/2011/OrganizationData.svc";
        var odataSelect = serverUrl + ODATA_ENDPOINT + "/" + object.odataSetName + "(guid'" + object.guid + "')";
        $.ajax({
            type: "GET",
            contentType: "application/json; charset=utf-8",
            datatype: "json",
            url: odataSelect,
            beforeSend: function (XMLHttpRequest) {
                XMLHttpRequest.setRequestHeader("Accept", "application/json");

            },
            success: function (data, textStatus, XmlHttpRequest) {
                var dataD = data.d;
                callback(dataD);
            },
            error: function (XmlHttpRequest, textStatus, errorObject) {

            }
        });

    }
    function XrmAttr(key) {
        console.log(key);
        var value = null;
        try{
            var value =   Xrm.Page.getAttribute(key).getValue();
        }catch (e){
            console.error("cannot get attr of "+key);
        }


        return {

            val:function (configOrFunction) {

                if(typeof configOrFunction == "function"){
                    return configOrFunction(value)
                }
                var opt;
                // if(typeof  configOrFunction == "object"){
                opt = $.extend({
                    default:"",
                    type:"string"
                },configOrFunction);
                // }


                if(!value){
                    if(opt.default != ""){
                        return opt.default;
                    }
                    if(opt.type == "string"){
                        return "";
                    }

                    if(opt.type == "int"){
                        return 0;
                    }
                }else{
                    if(opt.type == "string"){
                        return ""+value;
                    }
                }


                return value;
            },
            toString:function (handle) {

                if(handle){
                    var str = handle(value);
                    return ""+str;
                }
                if(!value){
                    return "";
                }
                return ""+value;
            },
            toInt:function (handle) {
                if(handle){
                    var str = handle(value);
                    return Number(str);
                }
                if(!value){
                    return 0;
                }
                return Number(str);
            },
            toDateTime:function(){
                if(!value){
                    return ""; //1900-01-01 00:00:00
                }
                return  formatDate(value)
            }

        }
    }

    return {
        getLookupValue: function (opt){
            var dfd = jQuery.Deferred();
            var cfg = $.extend({
                lookupAttributeName:"",
                lookupFieldName:"",
                odataSetName:"",
                targetAttributeName:"",
            },opt);
            console.log(cfg);
            var fieldvalue = Xrm.Page.getAttribute(cfg.lookupAttributeName).getValue();
            if (fieldvalue != null) {
                var fieldvalueId = fieldvalue[0].id;
                var objectOdata = new Object();
                objectOdata.odataSetName = cfg.odataSetName;
                objectOdata.guid = fieldvalueId;
                getOdataObject(objectOdata, function (result) {
                    var lookupFielValue = result[cfg.lookupFieldName];
                    console.log(cfg.lookupFieldName +"="+lookupFielValue);
                    dfd.resolve(lookupFielValue,result);
                    return lookupFielValue;
                });

            } else {
                dfd.reject(null);
                //return null;
            }

            return dfd.promise();
        },
        openWindow:function (opt) {
            var cfg  =  $.extend({
                // data sources
                iframe: true,
                inline: false,
                href: "",
                title:"",
                open:true,
                // callbacks
                onOpen: false,
                onLoad: false,
                onComplete: false,
                onCleanup: false,
                onClosed: false,
            },opt);


        },
        loadingStart:function () {
            LoadingStart();
        },
        loadingStop:function () {
            LoadingStop();
        },
        closeDialog:function () {
            CloseDialog();
        },
        alert:function (opt) {

            setTimeout(function () {
                // var $ = window.top.$;
                // console.log($("body").html);
                var option = {};

                if(typeof opt =="string"){
                    option = {
                        content:opt
                    }
                }else{
                    option = opt;
                }
                var cfg = $.extend({
                    title: "Alert box",
                    content: "",
                    type: "alert"
                }, option);





                OpenDialog(cfg);


            }, 500);
        },
        confirm:function (opt) {
            // var $ = window.top.$;
            // console.log($("body").html);
            var option = {};
            setTimeout(function () {
                if(typeof opt =="string"){
                    option = {
                        content:opt
                    }
                }else{
                    option  = opt;
                }
                var cfg = $.extend({
                    title: "Alert box",
                    content: "message",
                    type: "confirm"
                }, option);


                OpenDialog(cfg);


            }, 10);
        },
        error:function (opt) {
            var $ = window.top.$;
            if(typeof  opt == undefined){
                opt = "  ";
            }
            var option = {};
            setTimeout(function () {
                if(typeof opt =="string"){
                    option = {
                        content:opt,

                        type: "error"
                    }
                }else{

                    option = opt;
                }

                var cfg = $.extend({
                    title: "Error!!",
                    content: "",
                    type: "error",
                    document:document
                }, option);

                OpenDialog(cfg);


            }, 10);
        },
        attr:function  (key){
            return new XrmAttr(key,Xrm)
        },
        ajax:function (opt) {

            if(opt.api){
                opt.url =   "https://crmappqa.deves.co.th/XrmApi/Api/"+opt.api;
                if(window.location.hostname=="crmqa.deves.co.th"){
                    opt.url =   "https://crmappqa.deves.co.th/XrmApi/Api/"+opt.api;
                }

            }

            var cfg  =  $.extend({

                crossDomain: true,
                processData: false,
                method: "post",
                headers: {
                    Accept: "application/json",
                    "Content-Type": "application/json; charset=utf-8",
                    "Access-Control-Allow-Origin": '*'
                }
            },opt);

            if(cfg.data  && typeof cfg.data != "string"){
                cfg.data = JSON.stringify(cfg.data);
            }

            LoadingStart();
            var request = $.ajax(cfg);

            request.fail(function (jqXHR, textStatus,b,s) {
                LoadingStop();
                var responseObj = JSON.parse(jqXHR.responseText);
                var message = "";
                if(responseObj){
                    if(responseObj["Message"]){
                        message = responseObj["Message"];
                    }
                }{
                    message = "Request failed!!";
                }
                console.log([jqXHR, textStatus,b,s]);
                OpenDialog({title:textStatus,content:message,type:"error"});

            });
            request.always(function() {
                LoadingStop();
                console.log("always complete");
            });
            return request;
        }
    }
};

function UIController(resourceUrls, functionHandle) {



    if (!resourceUrls) {
        resourceUrls = new Array();
    }
    var resourceEndpoint = "https://crmappdev.deves.co.th/resources";

    //var resourceEndpoint = "";

    var commonResourceUrls = new Array();

    // commonResourceUrls.push(resourceEndpoint + '/jquery/dist/jquery.min.js');//https://crmappdev.deves.co.th/resources/jquery/dist/jquery.min.js
    // commonResourceUrls.push(resourceEndpoint + '/WebResources/pfc_Function_Appconfig');

    // commonResourceUrls.push(resourceEndpoint + '/common/app_config.js');



    if (!functionHandle) {
        console.log("NO FUNCTION");
        functionHandle = function (cfg, util) {

        }
    }

    var cfg = {};

    /**
     * Used  to inject a CSS link into the head element based on the url
     * @param url
     */
    function loadCss(url, callback) {
        //var document = window.top.document;
        var document = window.document;

        //  var file = location.pathname.split( "/" ).pop();

        var link = document.createElement("link");
        link.href = url;
        link.type = "text/css";
        link.rel = "stylesheet";
        link.media = "screen,print";

        link.onreadystatechange = callback;
        link.onload = callback;

        document.getElementsByTagName("head")[0].appendChild(link);
    }

    function loadScript() {

        function loadScript(url, callback) {
            //var document = window.top.document;
            var document = window.document;

            // Adding the script tag to the head as suggested before
            var head = document.getElementsByTagName('head')[0];
            var script = document.createElement('script');
            script.type = 'text/javascript';
            script.src = url;

            // Then bind the event to the callback function.
            // There are several events for cross browser compatibility.
            script.onreadystatechange = callback;
            script.onload = callback;

            // Fire the loading
            head.appendChild(script);
        }

    }

    function loadExternalResource(url, type) {
        console.log("load > " + url);
        return new Promise(function (resolve, reject) {

            var tag;
            var ltag;
            if (!type) {
                var match = url.match(/\.([^.]+)$/);
                if (match) {
                    type = match[1];
                }
            }

            if (!type) {
                type = "js";       // default to js
            }

            if (type === 'css') {
                tag = window.top.document.createElement("link");
                tag.type = 'text/css';
                tag.rel = 'stylesheet';
                tag.href = url;

                ltag = window.document.createElement("link");
                ltag.type = 'text/css';
                ltag.rel = 'stylesheet';
                ltag.href = url;
            }
            else if (type === "js") {
                tag = window.top.document.createElement("script");
                tag.type = "text/javascript";
                tag.src = url;

                ltag = window.document.createElement("script");
                ltag.type = "text/javascript";
                ltag.src = url;
            }
            if (tag) {
                tag.onload = function () {
                    console.log("resolve > " + url);
                    resolve(url);
                };
                tag.onerror = function () {
                    console.warn("reject > " + url);
                    reject(url);
                };
                window.top.document.getElementsByTagName("head")[0].appendChild(tag);

                ltag.onload = function () {
                    console.log("resolve > " + url);
                    resolve(url);
                };
                ltag.onerror = function () {
                    console.warn("reject > " + url);
                    reject(url);
                };
                window.document.getElementsByTagName("head")[0].appendChild(ltag);
            }
        });
    }

    function loadMultipleExternalResources(itemsToLoad) {
        var promises = itemsToLoad.map(function (url) {
            return loadExternalResource(url);
        });
        return Promise.all(promises);
    }

    function AppCommonUtil($,endpoint,document){
        function OpenDialog(option) {

            var cfg = $.extend({
                title: "Alert box",
                type: "alert",

                document:document,
                success: function (result) {
                },
                beforeShow: function () {
                },
                afterShow: function () {
                },
                beforeClose: function () {
                },
                afterClose: function () {
                },
            }, option);

            cfg.beforeShow();


            if($("body", window.top.document).find('#dialogAlertDivOverlay').length ==0){
                var elm = $('<div id="dialogAlertDivOverlay"><div  id="dialogAlertDiv"><p style="font-size: large;height:10px;"><img style=" width: 25px; vertical-align: middle;" src="data:image/png;base64," />'+cfg.title+'</p><hr/><div style="height:100px;" >'+cfg.content+'</div><div style="margin-top: 5px;"><hr/><button style="float: right;" id="alertOkButton">OK</button></div></div></div>');
                elm.css('visibility', 'visible');
                elm.css('position', 'absolute');
                elm.css('left', '0px');
                elm.css('top', '0px');
                elm.css('width', screen.width);
                elm.css('font-size', '1.5em');
                elm.css('height', screen.height);
                elm.css('text-align', 'center');
                elm.css('z-index', '1000');
                elm.css('background-color', 'rgba(224, 224, 224, 0.25)');


                elm.css('height', '100%');
                elm.css('margin', '0');
                elm.css('padding', '0');

                elm.find('#dialogAlertDiv').css('width', '500px');
                elm.find('#dialogAlertDiv').css('height', '200px');
                elm.find('#dialogAlertDiv').css('margin', '200px auto');
                elm.find('#dialogAlertDiv').css('background-color', '#fff');
                elm.find('#dialogAlertDiv').css('border', '1px solid #000');
                elm.find('#dialogAlertDiv').css('padding', '15px');
                elm.find('#dialogAlertDiv').css('padding-top', '0');
                elm.find('#dialogAlertDiv').css('text-align', 'center');

                $("body", window.top.document).append(elm);
            }



            $("body", window.top.document).find('#dialogAlertDivOverlay').css('visibility', 'visible');
            cfg.afterShow();
            $("body", window.top.document).find('#dialogAlertDivOverlay').find("#alertOkButton").unbind().bind("click",function () {
                cfg.beforeClose();
                $("body", window.top.document).find('#dialogAlertDivOverlay').css('visibility', 'hidden');
                cfg.afterClose();
            })

            cfg.success();
            //$("body", window.top.document).find('#dialogAlertDivOverlay').css('visibility', 'visible');
        }

        function LoadingStart() {

            if($("body", window.top.document).find('#dialogDiv').length ==0){


                var elm = $('<div id="overlay"><div id="dialogDiv"><p>Please wait</p><img src="data:image/gif;base64," /></div></div>');
                elm.css('visibility', 'hidden');
                elm.css('position', 'absolute');
                elm.css('left', '0px');
                elm.css('top', '0px');
                elm.css('width', screen.width);
                elm.css('height', screen.height);
                elm.css('text-align', 'center');
                elm.css('z-index', '1000');
                elm.css('background-color', 'rgba(224, 224, 224, 0.25)');


                elm.css('height', '100%');
                elm.css('margin', '0');
