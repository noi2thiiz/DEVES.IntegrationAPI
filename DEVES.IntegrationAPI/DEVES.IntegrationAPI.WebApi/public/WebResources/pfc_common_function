                setTimeout(function () {
                    if(typeof opt =="string"){
                        option = {
                            content:opt,
                            msgBoxImagePath:endpoint+"/msgbox"
                        }
                    }else{
                        option  = opt;
                    }
                    var cfg = $.extend({
                        title: "Alert box",
                        content: "message",
                        type: "confirm",
                        document:document
                    }, option);


                    OpenDialog(cfg);


                }, 10);
