            }
        }
    };

    loadMultipleExternalResources(commonResourceUrls).then(function () {
        console.log("then commonResourceUrls");
        loadMultipleExternalResources(resourceUrls).then(function () {
            console.log("then resourceUrls");

            var util = AppCommonUtil($,resourceEndpoint,window.top.document);
            var cfg = window.top.appConfig;
            functionHandle($, cfg, util,window);

        },function (url) {
            alert('cannot load resource : '+url);

        });

    },function (url) {
        alert('cannot load resource : '+url);
    });


}